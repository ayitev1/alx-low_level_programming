#include "main.h"

/**
 * print_to_98 - Entry point
 * @n: first operand ofparameters to add
 * this function is to print all natural numbers
 * from n to 98, followed by new line.
 *
 * Return: 0 Always - Success
 */

void print_to_98(int n)
{
	int i;

	for (i = n; i <= 98; i++)
		if (i != 98)
		{
			if (i >= 10)
			{
				_putchar((i / 10) + '0');
				_putchar((i % 10) + '0');
				_putchar(',');
				_putchar(' ');
			}
			else
			{
				_putchar(i + '0');
				_putchar(',');
				_putchar(' ');
			}
		}
		else
			_putchar(i + '0');
}
